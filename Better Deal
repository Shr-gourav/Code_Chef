#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int a,b;
	    cin>>a>>b;
	    int c= 100-a;
	    int d= 200-(b*2);
	    if(c<d){
	        cout<<"First"<<endl;
	    }
	    else if(c>d){
	        cout<<"Second"<<endl;
	    }
	    else{
	        cout<<"Both"<<endl;
	    }
	    
	}
	return 0;
}

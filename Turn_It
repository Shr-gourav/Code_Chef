#include <iostream>
#include<cmath>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    float u,v,a,s,ans;
	    cin>>u>>v>>a>>s;
	    ans=sqrt((u*u) - (2*a*s));
	    if(ans > v)
	    {
	        cout<<"No\n";
	    }
	    else
	    {
	        cout<<"Yes\n";
	    }
	}
	return 0;
}
